{
    "name": "cyphp/curl",
    "description": "A simple object oriented curl library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "cyveros",
            "email": "cyveros@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.0",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "symfony/var-dumper": "^3.2"
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit"
        ]
    },
    "autoload": {
        "psr-4": {
            "Cyphp\\Curl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cyphp\\Curl\\Test\\": "tests/"
        }
    }
}
